<script id="thumbnailAlbumTemplate" type="text/x-jquery-tmpl">
<div class="thumbnail albumThumbnail gridThumbnail" id="{{:identifier}}" data-link="data-pbdkey{:pbdkey} data-pbdoffset{:offset} data-size{:sizeClass} data-hover{:options.hoverEnabled}">
    <div class="thumbnailMedia">
        <div class="thumbnailImage"><a class="thumbnailLink album" href="{{:libraryUrl}}"><img data-thumbnailCount="4" src="{{:thumbUrl}}" alt="{{:altText}}" /></a></div>
        <div class="albumFilmstrip"><a class="thumbnailLink album" href="{{:libraryUrl}}"><img src="http://pic2.pbsrc.com/albums/filmstrip.png" height="141" width="600" alt="{{:title}}" /></a></div>

        <div class="thumbnailTitles">
            <p class="thumbnailTitle"><a class="thumbnailLink album" href="{{:libraryUrl}}">{{>title}}</a> <span class="mediaCount">({{>mediaCount}})</span></p>

            {{if options.showSubtitle}}
                <p class="thumbnailSubtitle">
                    {{>createdDateText}}

                    <div class="statsHandle"><i class="icon-info-sign icon-large"></i></div>
                </p>
            {{/if}}
        </div>

        {{if options.showSocialStats}}
            {{for #data tmpl="#socialStats" /}}
        {{/if}}

        {{if options.showAlbumCountBadge}}
            <div class="subalbumStats">{{>albumCountText}}</div>
        {{/if}}
    </div>

    {{if options.isDraggable}}
        <div class="dragHandle"></div>
    {{/if}}

    {{if options.showOptionsMenu}}
        <div class="optionsHandle"></div>

        <div class="optionsMenu" data-sharemode="album">
            <ul class="unstyled">
                {{if isOwner}}
                    <li><a href="#" class="editAlbumAction" data-parentalbumpath="{{:parentPath}}"
                                                            data-mode="edit"
                                                            data-albumpath="{{:path}}"
                                                            data-albumtitle="{{:title}}"
                                                            data-albumdescription="{{:description}}"
                                                            data-albumprivacylevel="{{:privacyLevel}}"
                                                            data-albumprivacypassword="{{:privacyPassword}}"
                                                            data-albumisroot="{{:isRoot}}">
                            {{>editAlbumAction}}
                        </a>
                    </li>
                {{/if}}

                <li><a href="javascript:void(0);" class="shareModalAction">{{>shareAlbumAction}}</a></li>

                {{if isOwner}}
                    <li><a href="/upload?location={{>location}}" class="albumUploadAction">{{>uploadToAlbumAction}}</a></li>
                   <!-- <li><a href="#">{{>createStorybucketAction}}</a></li>-->
                {{/if}}

                <li><a href="javascript:void(0);" class="shareModalAction" data-sharetype="links">{{>copyAlbumLinksAction}}</a></li>

                {{if isOwner}}
                    <li><a href="#" class="deleteAlbumAction" data-albumTitle="{{:title}}" data-albumPath="{{:path}}">{{>deleteAlbumAction}}</a></li>
                {{/if}}

                {{if isOwner}}
                    <li><a href="#" class="moveAlbumAction" data-albumTitle="{{:title}}" data-albumPath="{{:path}}">{{>moveAlbumAction}}</a></li>
                {{/if}}
                {{if isOwner}}
                    <li><a href="#" class="storyAddToModal" data-mode="album" data-albumpath="{{:path}}" data-validsource="{{:mediaCount}}"  onclick="Pb.Track.tr('story_add_to_click');Pb.Track.tr('story_add_to_albumthumbnail_click');" >{{>storyAddToModal}}</a></li>
                {{/if}}
            </ul>
        </div>
    {{/if}}

    {{if options.isBadged}}
        <div class="badgeContainer">
            <div class="badge {{>badgeClass}}"></div>
        </div>
    {{/if}}

    {{if options.showStats}}
        <div class="albumStats statsMenu">
            <h2>{{>albumStatsLabel}}</h2>

            <ul class="unstyled">
                <li class="statsImages">
                    <i class="icon-photo"></i>
                    {{>imageCountText}}
                </li>

                <li class="statsVideos">
                    <i class="icon-play-circle"></i>
                    {{>videoCountText}}
                </li>

                <li class="statsViews">
                    <i class="icon-eye-open"></i>
                    {{>pageViewsText}}
                </li>

                <li class="statsAlbums">
                    <i class="icon-albums"></i>
                    {{>albumCountText}}
                </li>

                {{if privacyLevel == 'public'}}
                    <li class="statsPublic">
                        <i class="icon-unlock"></i>
                        {{>publicAlbumLabel}}
                    </li>
                {{else}}
                    {{if hasPassword}}
                        <li class="statsPassword">
                            <i class="icon-key"></i>
                            {{>passwordAlbumLabel}}
                        </li>
                    {{else}}
                        <li class="statsPrivate">
                            <i class="icon-lock"></i>
                            {{>privateAlbumLabel}}
                        </li>
                    {{/if}}
                {{/if}}
                <!--<li class="statsGroup">{{>groupAlbumLabel}}</li>-->
            </ul>

            <div style="clear: both"></div>
        </div>
    {{/if}}
</div>
</script>

<script id="albumStatsTemplate" type="text/x-jquery-tmpl">
<ul class="unstyled">
    {{for albumStats tmpl="#albumStatsItemTemplate"/}}
</ul>
</script>

<script id="albumStatsItemTemplate" type="text/x-jquery-tmpl">
    {{if images}}
    <li class="statsImages">
        <i class="icon-photo"></i>{{>images.display}}
    </li>
    {{/if}}
    {{if videos}}
    <li class="statsVideos">
        <i class="icon-play-circle"></i>{{>videos.display}}
    </li>
    {{/if}}
    {{if slideshows}}
    <li class="statsSlideshow">
        <i class="icon-slideshow"></i>{{>slideshows.display}}
    </li>
    {{/if}}
    {{if views}}
    <li class="statsViews">
        <i class="icon-eye-open"></i>{{>views.display}}
    </li>
    {{/if}}
    {{if subalbums}}
    <li class="statsAlbums">
        <i class="icon-albums"></i>{{>subalbums.display}}
    </li>
    {{/if}}
    {{if privacy}}
        {{if privacy.isPublic}}
        <li class="statsPublic">
            <i class="icon-unlock"></i>{{>privacy.display}}
        </li>
        {{else}}
            {{if privacy.hasPassword}}
                <li class="statsPassword">
                    <i class="icon-key"></i>{{>privacy.display}}
                </li>
            {{else}}
                <li class="statsPrivate">
                    <i class="icon-lock"></i>{{>privacy.display}}
                </li>
            {{/if}}
        {{/if}}
    {{/if}}
</script>
<script 
    id="connectionItem"
    type="text/x-jquery-tmpl">  
    {{if connectMethod === 'checkbox'}}
        <input type="checkbox" id="service_{{:service}}" data-connecturl="/auth/{{:service}}/start?callback_type=connect" name="service_{{:service}}" />
    {{/if}}
    {{if isConnected }}
        <div class="socialicon connected pull-left">
            <div class="{{:service}}-image "></div>
            <img src="{{:siteUserImage}}" alt="" />
        </div>
    {{else}}
        <div class="socialicon {{:service}}-icon pull-left"></div>
    {{/if}}

    {{if style === 'verbose'}}
        {{if connectMethod === 'link'}}
            {{for actionBtn tmpl="#buttonTemplate"/}} 
        {{/if}}
    
        {{if connectMethod === 'checkbox'}}
            <label for="service_{{:service}}">
                {{>connectionLabel}}
            </label>
        {{else}}
            <div class='connectionDetails'>
                <p>
                    <span class="site-name">{{>serviceDisplayName}}{{if isConnected}}:{{/if}}</span>
                    {{if isConnected}}
                        <a href="{{:siteUserLink}}" target="_blank">{{:connectionLabel}}</a>
                    {{/if}}
                </p>
                {{if isConnected && autoshare}}
                    <div class='autoShare insetBox'>
                        {{for autoshare tmpl="#checkboxTemplate" /}}
                    </div>
                {{/if}}
            </div>
        {{/if}}            

 
        {{if connectMethod !== 'link'}}
            {{>connectionLabel}}
        {{/if}}
    {{else connectMethod === 'checkbox'}}
        <label for="service_{{:service}}">
            {{>connectionLabel}}
        </label>
    {{else isConnected}}
        {{>connectionLabel}}
    {{else}}
        <a href="/auth/{{:service}}/start?callback_type=connect" class="connectLink" data-connected="{{:isConnected ? 'true' : 'false'}}">
            {{>connectionLabel}}
        </a>
    {{/if}}            
</script>
<script id="checkboxTemplate" type="text/x-jquery-tmpl">
<fieldset class="control-group">
    {{if label}}
        <span class="control-label">{{>label}}</span> 
    {{/if}}
    <div id="{{:id}}" class="controls">
        {{for boxes}}
            <label class="checkbox inline" for="{{:id}}">
                <input type='checkbox' name="{{:#parent.parent.data.name}}" id="{{:id}}" value="{{:value}}" />
                {{>label}}
            </label>
        {{/for}}
    </div>
</fieldset>
</script>
<script id="copyCodeTemplate" type='text/x-jquery-tmpl'>
<div class="zeroclipboard" data-link="id{:zclipId} data-tracking{:tracking}">
    <div class="copycode control-group" data-link="id{:cclipboardId}">
        <label class="link" data-link="inputLabel"></label>
        <input class="linkcopy" readonly="readonly" type="text" data-link="id{:ccInputId} value{:inputText}" />
        <div class="ccplaceholder"></div>
        <div class="clearB"></div>
    </div>
</div>
</script>
<!-- Template for a Media object. -->
<script id="thumbnailMediaTemplate" type="text/x-jquery-tmpl">
<div data-mediaType="{{:mediaType}}" data-link="id{:id} class{:sizeClass+' thumbnail imageThumbnail gridThumbnail'} data-pbdkey{:pbdkey} data-pbdoffset{:offset} data-size{:sizeClass} data-hover{:options.hoverEnabled}">
    <div class="thumbnailMedia">
        <div class="thumbnailImage">
            <a class="thumbnailLink media" data-link="href{:linkUrl}">
                <img data-link="src{:thumbUrl} alt{:alt}" />
                <div class="thumbnailOverlay"></div>
            </a>
        </div>

        <div class="thumbnailTitles">
            <p class="thumbnailTitle"><a data-link="{:title} href{:linkUrl}"></a>&nbsp;</p>

            {{if options.showSubtitle}}
                <p class="thumbnailSubtitle" data-link="subtitleText"></p>
            {{/if}}
        </div>

        {{if options.showSocialStats}}
            {{for #data tmpl="#socialStats" /}}
        {{/if}}
    </div>

    {{if options.isDraggable}}
        <div class="dragHandle"></div>
    {{/if}}

    {{if options.showOptionsMenu}}
        <div class="optionsHandle"></div>

        <div class="optionsMenu">
            <ul class="unstyled" data-sharemode="media">
                <li><a href="javascript:void(0);" class="shareModalAction" data-link="shareMediaAction"></a></li>
                
                {{if cd}}
                <li><a href="#" class="copyMoveMediaAction" data-copymovemode="copy" data-link="copyMediaAction"></a></li>
                {{/if}}

                {{if isOwner}}
                    {{if mediaType == 'image'}}
                        <li><a href="#" class="editMediaAction" data-link="editMediaAction"></a></li>
                    {{/if}}
                    <li><a href="#" class="copyMoveMediaAction" data-copymovemode="move" data-link="moveMediaAction"></a></li>
                {{/if}}

                <li><a href="javascript:void(0);" class="shareModalAction" data-sharetype="links" data-link="copyMediaLinksAction"></a></li>

                {{if isOwner}}
                    <li><a href="#" class="deleteMediaAction" data-refresh="false" data-link="deleteMediaAction"></a></li>
                {{/if}}
            </ul>
        </div>
    {{/if}}

    {{if options.isBadged}}
        <div class="badgeContainer">
            <div class="badge {{>badgeClass}}"></div>
        </div>
    {{/if}}
</div>
{{if options.insertBanner}}
    <div class="ad bannerAd {{:options.insertBanner}} contentBannerAd"></div>
    <div class="clearB"></div>
{{/if}}
</script>

<script id="socialStats" type="text/x-jquery-tmpl">
    <div class="socialStats">
        <ul class='unstyled'>
            <li class="icon-comment" data-link="{>commentCount}"></li>
            <li class="icon-heart" data-link="{>likeCount}"></li>
        </ul>
    </div>
</script>

<script id="dropdownTemplate" type="text/x-jquery-tmpl">
<ul class="unstyled dropdownContainer {{:className}}" id="{{:id}}">
    <li class="dropdown">
        <a href="#" class="btn btn-small btn-secondary dropdown-toggle" data-toggle="dropdown">
            {{if selectedItem}}
                {{>selectedItem.title}}
            {{else}}
                {{>emptyLabel}}
            {{/if}}
            <b class="caret"></b>
        </a>
        <ul class="dropdown-menu">
            {{for items tmpl="#dropdownItemTemplate" /}}
        </ul>
    </li>
</ul>
</script>

<script id="dropdownItemTemplate" type="text/x-jquery-tmpl">
<li class="{{:liClass}}">
    <a class="{{:itemClass}}" href="{{:href}}" {{:attributes}}>{{>title}}</a>
</li>
</script>
<script id="breadcrumbsItemTemplate" type="text/x-jquery-tmpl">
{{if ~isCurrentBreadcrumb(link)}}
    <li>{{>label}}</li>
{{else}}
    <li>
        <a data-link="href{:link}">{{>label}}</a>
        <span class="divider">&#47;</span>
    </li>
{{/if}}
</script>

<script id="breadcrumbsTemplate" type="text/x-jquery-tmpl">
{{for breadcrumbs tmpl="#breadcrumbsItemTemplate" /}}
</script>

<script
    id="alertTemplate"
    type="text/x-jquery-tmpl">
    <div class="global alert fade slide
        alert-{{:messageClass}}"
        data-alert="alert">
        <i class="icon-{{:messageIcon}} icon-large"></i>
        <a class="close" data-dismiss="alert" href="#"><i class='icon-remove'></i></a>
        <div class="msg-body">
            <div class="msg-title">{{:message}}</div>
            <div class="msg-details">{{:details}}</div>
        </div>
    </div>
</script>

<script type="text/javascript" src="http://static2.pbsrc.com/pkg/220a61b3e583/deferred_global_main.js" ></script>
<script type="text/javascript" src="http://static2.pbsrc.com/pkg/220a61b3e583/deferred_explore_explorepage.js" ></script>

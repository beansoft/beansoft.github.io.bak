<script id="stopwatchesTemplate" type="text/x-jquery-tmpl">
    <tr>
        <th colspan="2">{{:url}}</td>
    </tr>
    <tr>
        <td>{{:stopwatch.name}} ({{:~round(stopwatch.elapsed)}} s)</td>
